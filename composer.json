{
    "name": "masoud5070/laravel-oci8",
    "description": "Oracle DB driver for Laravel 4|5|6|7 via OCI8",
    "keywords" : ["laravel","oracle","oci8","pdo_oci"],
    "license": "MIT",
    "authors": [
        {
            "name": "Masoud Haji Ali Tajer",
            "email": "masoudtajer121@gmail.com",
            "homepage": "https://github.com/masoud5070",
            "role": "Forker of yajra/laravel-oci8"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-oci8": ">=2.0.0",
        "ext-pdo": "*",
        "illuminate/database": "^8",
        "illuminate/pagination": "^8",
        "illuminate/support": "^8",
        "illuminate/validation": "^8",
        "mahdiazadbar/laravel-pdo-via-oci8": "^2.0|^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.5",
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "files": [
          "src/helper.php"
        ],
        "psr-4": {
            "Masoud5070\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Masoud5070\\Oci8\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
          "dev-master": "8.x-dev"
        },
        "laravel": {
            "providers": [
                "Masoud5070\\Oci8\\Oci8ServiceProvider",
                "Masoud5070\\Oci8\\Oci8ValidationServiceProvider"
            ]
        }
    },
    "scripts": {
        "docker": "docker run -d -p 49160:22 -p 49161:1521 deepdiver/docker-oracle-xe-11g"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
